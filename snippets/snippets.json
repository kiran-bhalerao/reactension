	"Class Component": {
		"prefix": "rcom",
		"body": [
			"class ${1:${TM_FILENAME_BASE}} extends Component {",
			"    render() {",
			"        return(",
			"            <div>",
			"                This is ${1:${TM_FILENAME_BASE}} component.",
			"            </div>",
			"        );",
			"    }",
			"}"
		],
		"description": "Class Component !"
